<?xml version='1.0' encoding='utf-8'?>
<widget id="me.tehsis.rabbitwars" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Rabbit Wars</name>
    <description>
      Save the rabbit planet!
    </description>
    <author email="tehsis@gmail.com" href="http://tehsis.me">
      Pablo Teradillos
    </author>
    <author email="bunny@gmail.com" href="http://tehsis.me">
      Yohana Etchemendy
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*facebook.com*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-facebook4" spec="~1.7.4">
        <variable name="APP_ID" value="358865204487691" />
        <variable name="APP_NAME" value="rabbit wars" />
    </plugin>
</widget>
